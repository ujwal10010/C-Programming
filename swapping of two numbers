#include<stdio.h>
int main(){
    int a,b;
    printf("Enter number a : ");
    scanf("%d",&a);
    printf("Enter number b : ");
    scanf("%d",&b);
    printf("The number a = %d \nThe number b = %d\n",a,b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("Now the swaped numbers are\nThe number a = %d \nThe number b = %d",a,b);
    return 0;
}
