#include<stdio.h> 
int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
        int i,x;
    scanf("%d",&x);
    int list[x];
    for(i=1;i<=x;i=i+1){
        scanf("%d",&list[i]);
    }
  for(i=x;i>=1;i--){
                printf("%d ",list[i]);
  }
          
    return 0;
}
