#include<stdio.h>
int main(){
    int y,day,days,x,z,a,b,c;
    printf("Enter your total amount: ");
    scanf("%d",&day);
        printf("500: %d \n",day/500);
        days = day%500;
        printf("100: %d \n",days/100);
        x = days % 100;
        printf("50: %d \n",x/50);
        y = x % 50;
        printf("20: %d \n",y/20);
        z = y % 20;
        printf("10: %d \n",z/10);
        a = z % 10;
        printf("5: %d \n",a/5);
        b = a % 5;
        printf("2: %d \n",b/2);
        c = b % 2;
        printf("1: %d \n",c/1);
    return 0;
}
