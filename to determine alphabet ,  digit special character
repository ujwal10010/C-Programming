#include<stdio.h>
int main(){
    char ch;
    printf("Enter your character: ");
    scanf("%c",&ch);
    if((ch>=65 && ch<=90) || (ch>=97 && ch<=122))
    printf("%c is Alphabet\n",ch);
    else if(ch>=48 && ch<=57)
    printf("%c is Digit\n",ch);
    else
    printf("%c is Special Character\n",ch);
return 0;
}
